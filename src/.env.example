# HabitWire - Development Environment
# Copy to .env and adjust values
# Only needed for local development (npm run dev)

# Database
DB_HOST=localhost
DB_PORT=5432
DB_NAME=habitwire
DB_USER=habitwire
DB_PASSWORD=habitwire_dev

# Session Secret (min 32 characters)
# Generate with: openssl rand -base64 32
NUXT_SESSION_PASSWORD=change-me-to-at-least-32-characters

# Initial Admin User (only on first startup)
INITIAL_USER=admin
INITIAL_PASSWORD=change-me

# Rate Limiting (requests per minute)
RATE_LIMIT_PER_MINUTE=1000
